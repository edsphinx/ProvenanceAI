# Environment Variables for Provenance AI
# Copy this file to .env and fill in your actual values

# ==========================================
# AI Generation API
# ==========================================
DEEPSEEK_API_KEY=sk-your-deepseek-api-key-here

# Alternative: Replicate API (if using Replicate instead)
# REPLICATE_API_KEY=r8_your-replicate-api-key-here

# ==========================================
# Story Protocol Configuration
# ==========================================
# Your wallet private key for deploying the parent AI Model IP
STORY_WALLET_PRIVATE_KEY=0x...

# Story RPC (already configured, but can override)
STORY_RPC_URL=https://testnet.storyrpc.io

# ==========================================
# Constellation Network
# ==========================================
# P12 File Passwords (KEEP THESE SECRET!)
CONSTELLATION_GENESIS_PASSWORD=your-genesis-password-here
CONSTELLATION_VALIDATOR_PASSWORD=your-validator-password-here
CONSTELLATION_TOKEN_KEY_PASSWORD=your-token-password-here

# Metagraph Configuration
CONSTELLATION_NETWORK=integrationnet
CONSTELLATION_METAGRAPH_ID=your-metagraph-id-here

# Data L1 Endpoint (after deployment, update with your VPS IP)
CONSTELLATION_DATA_L1_URL=http://localhost:9400/data

# Global L0 Endpoint (testnet)
CONSTELLATION_GLOBAL_L0_URL=https://l0-lb-testnet.constellationnetwork.io

# ==========================================
# ICP Deployment
# ==========================================
# Network to deploy to (local or ic)
DFX_NETWORK=local

# ==========================================
# Frontend (React)
# ==========================================
# Web3.storage API token for dispute evidence
WEB3_STORAGE_TOKEN=your-web3-storage-token

# ==========================================
# Testing
# ==========================================
# Set to true to use stub AI generation (no API calls)
USE_AI_STUB=false
